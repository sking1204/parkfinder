{
    "$schema": "https://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "title": "Root Schema",
    "type": "object",
    "default": {},
    "required": [],
    "properties": {
        "password": {
            "title": "The password Schema",
            "type": "string",
            "default": "",
            "minLength": 5,
            "maxLength": 20,
            "examples": [
                "password!"
            ]
        },
        "firstName": {
            "title": "The firstName Schema",
            "type": "string",
            "default": "",
            "minLength": 1,
            "maxLength": 30,
            "examples": [
                "Test"
            ]
        },
        "lastName": {
            "title": "The lastName Schema",
            "type": "string",
            "default": "", 
            "minLength": 1,
            "maxLength": 30,           
            "examples": [
                "User4"
            ]
        },
        "email": {
            "title": "The email Schema",
            "type": "string",
            "default": "",
            "minLength": 6,
            "maxLength": 60,
            "format": "email",
            "examples": [
                "testuser4@email.com"
            ]
        }
    },
    "examples": [{
        "password": "password!",
        "firstName": "Test",
        "lastName": "User4",
        "email": "testuser4@email.com"
    }]
}